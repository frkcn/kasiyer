{
    "name": "frkcn/kasiyer",
    "description": "Kasiyer makes Iyzico's subscription billing services easy and fun to use",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Faruk Can",
            "email": "faarukcan@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "illuminate/contracts": "^6.0|^7.0",
        "illuminate/database": "^6.0|^7.0",
        "illuminate/http": "^6.0|^7.0",
        "illuminate/log": "^6.0|^7.0",
        "illuminate/notifications": "^6.0|^7.0",
        "illuminate/routing": "^6.0|^7.0",
        "illuminate/support": "^6.0|^7.0",
        "illuminate/view": "^6.0|^7.0",
        "moneyphp/money": "^3.2",
        "nesbot/carbon": "^2.0",
        "symfony/http-kernel": "^4.3|^5.0",
        "symfony/intl": "^5.0",
        "iyzico/iyzipay-php": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0@dev",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.8|^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Frkcn\\Kasiyer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Frkcn\\Kasiyer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Frkcn\\Kasiyer\\KasiyerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
